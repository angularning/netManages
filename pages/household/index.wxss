page{
  background: #f9f9f9;
}
.houseTitle{
  font-size: 16px;
  color: #333;
  position: relative;
  padding-left: 10px;
  margin: 20px 12px;
}
.baseBody{
  margin: 0 12px;
}
.houseTitle::after{
  content: '';
  position: absolute;
  width: 4px;
  height: 20px;
  left: 0;
  top: 2px;
  background: #1989fa;
}
.baseCell{
  background: #fff;
  padding: 12px 0;
  border-bottom: 1px #eee solid;
  display: flex;
  flex-flow: left;
  font-size: 14px;
}
.baseCell .name{
  color: #333;
  width: 120px;
  margin-left: 12px;
}

.baseCell .value{
  color: #999;
  margin-right: 12px;
  width: calc(100% - 100px);
  margin-left: 10px;
}


.extralCell{
  background: #fff;
  padding: 12px 0;
  border-bottom: 1px #eee solid;
  display: flex;
  flex-flow: left;
  font-size: 14px;
}
.extralCell .name{
  color: #333;
  width: 94px;
  margin-left: 12px;
}

.extralCell .value{
  color: #999;
  margin-right: 12px;
  width: calc(100% - 100px);
  margin-left: 10px;
}
.cellIcon image{
  width: 24px;
  height: 24px;
  position: relative;
  right: -6px;
  top: -1px;
  vertical-align: middle;
}