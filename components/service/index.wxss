
.indexRightIcon{
  position: fixed;
  right: 3%;
  bottom: 100px;
}
.indexRightIcon image{
  width: 50px;
  height: 50px;
  box-shadow: 0px 3px 8px 0px rgba(96, 96, 96, 0.2);
  border-radius: 50%;
}
.indexRightIcon view{
  text-align: center;
  margin-bottom: 10px;
}
.indexRightIcon view button{
  background: none;
  border: none;
  background-color: none;
}
.indexRightIcon view button::after{
  border: none;
}
.indexRightIcon view view{
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #1C2D5A;
  font-size: .85rem;
}